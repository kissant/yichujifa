<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="#d0fbbc"/>
            <stroke android:width="1dp" android:color="#20696969"/>
            <padding android:left="8dp" android:bottom="3dp" android:right="8dp" android:top="3dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="#d0fbbc"/>
            <stroke android:width="1dp" android:color="#20696969"/>
            <padding android:left="8dp" android:bottom="3dp" android:right="8dp" android:top="3dp"/>
        </shape>
    </item>
</selector>